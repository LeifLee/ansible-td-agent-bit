{
  "ID": "{{ ansible_hostname }}",
  "Name": "{{ ansible_hostname }}",
  "Tags": ["{{ consul_tags | list | join ('\",\"') }}"],
  "Address": "{{ ansible_eth0['ipv4']['address'] }}",
  "Port": 2020,
  "EnableTagOverride": false,
  "Check": {
    "DeregisterCriticalServiceAfter": "5m",
    "TCP": "{{ ansible_eth0['ipv4']['address'] }}:2020",
    "Interval": "10s"
  },
  "Weights": {
    "Passing": 10,
    "Warning": 1
  }
}
